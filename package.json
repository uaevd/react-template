        "eslint": "^8.54.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-storybook": "^0.6.15",
        "lefthook": "^1.5.4",
        "prettier": "^3.1.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
